<?xml version = '1.0' encoding = 'windows-1252'?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0">
    <servlet>
        <servlet-name>AccountDetailsEndpointSOAP11Port</servlet-name>
        <servlet-class>com.hdfc.netbanking.services.AccountDetailsEndpointImpl</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>AccountDetailsEndpointSOAP11Port</servlet-name>
        <url-pattern>/AccountDetailsEndpointSOAP11Port</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>AccountDetailsEndpointSOAP12Port</servlet-name>
        <servlet-class>com.hdfc.netbanking.services.AccountDetailsEndpoint12Impl</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>AccountDetailsEndpointSOAP12Port</servlet-name>
        <url-pattern>/AccountDetailsEndpointSOAP12Port</url-pattern>
    </servlet-mapping>
</web-app>
