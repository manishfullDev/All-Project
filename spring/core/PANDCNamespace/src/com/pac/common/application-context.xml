<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:c="http://www.springframework.org/schema/c"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="machine" class="com.pac.beans.Machine" c:capacity="100"
		c:machineType="Normal" />

	<bean id="toy" class="com.pac.beans.Toy" p:toyName="Car"
		p:ageGroup="5" p:manufacturer="hot wheels" p:color="red" p:price="550"
		p:machine-ref="machine" />

</beans>
