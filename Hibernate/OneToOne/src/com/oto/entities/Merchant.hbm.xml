<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.oto.entities">
	<class name="Merchant" table="merchant">
		<id name="merchantNo" column="merchant_no">
			<generator class="increment" />
		</id>
		<property name="businessName" column="business_nm" />
		<property name="businessLicenseNo"
			column="business_license_no" />
		<property name="establishedDate" column="established_dt" />
		<property name="businessType" column="business_type" />
		<property name="ownerName" column="owner_nm" />
		<property name="businessContactNo"
			column="business_contact_no" />
		<property name="businessEmailAddress"
			column="business_email_address" />
		<property name="status" />
	</class>
</hibernate-mapping>