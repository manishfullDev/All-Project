<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.msf.entities">
	<class name="Vehicle" table="vehicle">
		<id name="vehicleNo" column="vehicle_no" />

		<property name="modelName" type="string">
			<column name="model_nm" length="50" not-null="true"
				unique="false" sql-type="VARCHAR" />
		</property>

		<property name="registrationNo" column="registration_no" />
		<property name="engineNo" column="engine_no" />
		<property name="chasisNo" column="chasis_no" />
		<property name="color" />
		<property name="fuelType" column="fuel_type" />
		<property name="capacity" />
	</class>
</hibernate-mapping>






