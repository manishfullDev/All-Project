<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.mtml.entities">
	<class name="Participant" table="participant">
		<id name="participantNo" column="participant_no">
			<generator class="increment" />
		</id>
		<property name="participantName" column="participant_nm" />
		<property name="age" column="age" />
		<property name="gender" column="gender" />
		<property name="contactNo" column="contact_no" />
		<property name="emailAddress" column="email_address" />
	</class>
</hibernate-mapping>