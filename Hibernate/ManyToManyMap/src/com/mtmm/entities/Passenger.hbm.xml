<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.mtmm.entities">
	<class name="Passenger" table="passenger">
		<id name="passengerNo" column="passenger_no">
			<generator class="increment" />
		</id>
		<property name="passengerName" column="passenger_nm" />
		<property name="age" />
		<property name="gender" />
		<property name="mobileNo" column="mobile_nbr" />
		<property name="emailAddress" column="email_address" />
	</class>
</hibernate-mapping>