<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<filter>
		<filter-name>SessionTokenizer</filter-name>
		<filter-class>com.df.filter.SessionTokenizerFilter</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>SessionTokenizer</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>
	<servlet>
		<servlet-name>openAccount</servlet-name>
		<servlet-class>com.df.servlet.OpenAccountServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>openAccount</servlet-name>
		<url-pattern>/openAccount</url-pattern>
	</servlet-mapping>
</web-app>
